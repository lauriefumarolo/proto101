{"components/common/share":"{{#if layout '==' 'product-view'}}\n    <div class=\"shareLinkSocial{{#unless product.can_purchase}} product-outOfStock{{/unless}}\">\n        <a class=\"shareLinkSocial__button\" href=\"javascript:void(0)\" aria-label=\"Open Popup Share\">\n            <svg class=\"icon\" aria-label=\"icon Share\">\n                <title>icon Share</title>\n                <desc>icon Share</desc>\n                <use xlink:href=\"#icon-share\"></use>\n            </svg>\n        </a>\n        <div class=\"shareLinkSocial__popup\" style=\"display: none\">\n            <div class=\"shareLinkSocial__link\">\n                <p class=\"title\">{{lang 'common.copy_link'}}</p>\n                <input id=\"shareLinkSocial__copy\" type=\"text\" value=\"{{product.url}}\">\n                <a href=\"javascript:void(0)\" class=\"shareLinkSocial__close\" aria-label=\"Close Popup Share\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></svg>\n                </a>\n            </div>\n            {{assignVar 'encodedUrl' (encodeURI url)}}\n            {{assignVar 'encodedTitle' (encodeURI head.title)}}\n            <div class=\"shareLinkSocial__social\">\n                <ul class=\"socialLinks\">\n                    {{#each settings.add_this.buttons}}\n                        {{#if service '===' 'facebook'}}\n                            <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                                <a class=\"socialLinks__link icon icon--{{service}}\"\n                                    title=\"{{{capitalize service}}}\"\n                                    href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                                    target=\"_blank\"\n                                    rel=\"noopener\"\n                                    {{{annotation}}}\n                                >\n                                    <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                    <svg>\n                                        <use href=\"#icon-facebook\"/>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{else if service '===' 'email'}}\n                            <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                                <a class=\"socialLinks__link icon icon--{{service}}\"\n                                    title=\"{{{capitalize service}}}\"\n                                    href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                                    target=\"_self\"\n                                    rel=\"noopener\"\n                                    {{{annotation}}}\n                                >\n                                    <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                    <svg>\n                                        <use href=\"#icon-envelope\"/>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{else if service '===' 'print'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                title=\"{{{capitalize service}}}\"\n                                onclick=\"window.print();return false;\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use href=\"#icon-print\"/>\n                                </svg>\n                            </a>\n                        </li>\n                        {{else if service '===' 'twitter'}}\n                            <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                                <a class=\"socialLinks__link icon icon--{{service}}\"\n                                    href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                                    target=\"_blank\"\n                                    rel=\"noopener\"\n                                    title=\"X\"\n                                    {{{annotation}}}\n                                >\n                                    <span class=\"aria-description--hidden\">X</span>\n                                    <svg>\n                                        <use href=\"#icon-x\"/>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{else if service '===' 'linkedin'}}\n                            <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                                <a class=\"socialLinks__link icon icon--{{service}}\"\n                                    title=\"{{{capitalize service}}}\"\n                                    href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                                    target=\"_blank\"\n                                    rel=\"noopener\"\n                                    {{{annotation}}}\n                                >\n                                    <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                    <svg>\n                                        <use href=\"#icon-linkedin\"/>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{else if service '===' 'pinterest'}}\n                            <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                                <a class=\"socialLinks__link icon icon--{{service}}\"\n                                    title=\"{{{capitalize service}}}\"\n                                    href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;media={{getImageSrcset ../product.main_image (cdn ../theme_settings.default_image_product) 1x=../theme_settings.zoom_size}}&amp;description={{getVar 'encodedTitle'}}\"\n                                    target=\"_blank\"\n                                    rel=\"noopener\"\n                                    {{{annotation}}}\n                                >\n                                    <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                    <svg>\n                                        <use href=\"#icon-pinterest\"/>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{/if}}\n                    {{/each}}\n                </ul>\n            </div>\n            {{#if settings.facebook_like_button.enabled}}\n                <iframe\n                    class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n                    style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\n                    src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n                    scrolling=\"no\"\n                    frameborder=\"0\"\n                    allowTransparency=\"true\">\n                </iframe>\n            {{/if}}\n        </div>\n    </div>\n{{else}}\n    {{#if settings.add_this.buttons}}\n        {{assignVar 'encodedUrl' (encodeURI url)}}\n        {{assignVar 'encodedTitle' (encodeURI head.title)}}\n        <div>\n            <ul class=\"socialLinks\">\n                {{#each settings.add_this.buttons}}\n                    {{#if service '===' 'facebook'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                title=\"{{{capitalize service}}}\"\n                                href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                                target=\"_blank\"\n                                rel=\"noopener\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use href=\"#icon-facebook\"/>\n                                </svg>\n                            </a>\n                        </li>\n                    {{else if service '===' 'email'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                title=\"{{{capitalize service}}}\"\n                                href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                                target=\"_self\"\n                                rel=\"noopener\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use href=\"#icon-envelope\"/>\n                                </svg>\n                            </a>\n                        </li>\n                    {{else if service '===' 'print'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a class=\"socialLinks__link icon icon--{{service}}\"\n                            title=\"{{{capitalize service}}}\"\n                            onclick=\"window.print();return false;\"\n                            {{{annotation}}}\n                        >\n                            <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                            <svg>\n                                <use href=\"#icon-print\"/>\n                            </svg>\n                        </a>\n                    </li>\n                    {{else if service '===' 'twitter'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                                target=\"_blank\"\n                                rel=\"noopener\"\n                                title=\"X\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">X</span>\n                                <svg>\n                                    <use href=\"#icon-x\"/>\n                                </svg>\n                            </a>\n                        </li>\n                    {{else if service '===' 'linkedin'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                title=\"{{{capitalize service}}}\"\n                                href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                                target=\"_blank\"\n                                rel=\"noopener\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use href=\"#icon-linkedin\"/>\n                                </svg>\n                            </a>\n                        </li>\n                    {{else if service '===' 'pinterest'}}\n                        <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                            <a class=\"socialLinks__link icon icon--{{service}}\"\n                                title=\"{{{capitalize service}}}\"\n                                href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;media={{getImageSrcset ../product.main_image (cdn ../theme_settings.default_image_product) 1x=../theme_settings.zoom_size}}&amp;description={{getVar 'encodedTitle'}}\"\n                                target=\"_blank\"\n                                rel=\"noopener\"\n                                {{{annotation}}}\n                            >\n                                <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                                <svg>\n                                    <use href=\"#icon-pinterest\"/>\n                                </svg>\n                            </a>\n                        </li>\n                    {{/if}}\n                {{/each}}\n            </ul>\n        </div>\n    {{/if}}\n    {{#if settings.facebook_like_button.enabled}}\n    <iframe\n        class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n        style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\n        src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n        scrolling=\"no\"\n        frameborder=\"0\"\n        allowTransparency=\"true\">\n    </iframe>\n    {{/if}}\n{{/if}}\n","components/common/wallet-buttons":"{{#if wallet_buttons}}\n    {{{wallet_buttons}}}\n{{/if}}\n","components/products/add-to-cart":"<div class=\"alertBox alertBox--error productAttributes-message\" style=\"display:none\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\"></p>\n</div>\n<div class=\"productView-actions\">\n    <div class=\"form-field form-field--increments\">\n        <label class=\"form-label\" for=\"qty[]\">{{lang 'products.quantity'}}</label>\n        <div class=\"form-increment\" data-quantity-change>\n            <button class=\"button button--icon\" data-action=\"dec\">\n                <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\n            </button>\n            <input class=\"form-input form-input--incrementTotal\"\n                id=\"qty[]\"\n                name=\"qty[]\"\n                type=\"number\"\n                value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\n                data-quantity-min=\"{{product.min_purchase_quantity}}\"\n                data-quantity-max=\"{{product.max_purchase_quantity}}\"\n                min=\"1\"\n                pattern=\"[0-9]*\"\n                aria-live=\"polite\"\n            >\n            <button class=\"button button--icon\" data-action=\"inc\">\n                <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\n            </button>\n        </div>\n    </div>\n    <div class=\"form-actionFix\">\n        {{#or customer (unless settings.hide_price_from_guests)}}\n            <div class=\"form-action\" role=\"status\" aria-live=\"polite\">\n                <input id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--secondary\" type=\"submit\"\n                    value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\" {{#if product.can_purchase}}{{else}}disabled {{/if}}>\n            </div>\n        {{else}}\n            <div class=\"form-action\">\n                <span class=\"form-action-text\">\n                    <span class=\"text\">{{lang 'common.restrict_to_login'}}</span>\n                </span>\n            </div>\n        {{/or}}\n        {{#if settings.show_wishlist}}\n            <div class=\"card-wishlist\" data-wishlist-add>\n                <button class=\"js-wishlist\" data-id=\"{{product.id}}\" aria-label=\"Wishlist\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-heart\" /></svg>\n                </button>\n                <ul class=\"wishlist-menu has-loading haloCustomScrollbar animated--out\" data-halo-animate=\"1\">\n                    <li class=\"wishlist-item\">\n                        <a href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">\n                            <span>{{lang \"account.wishlists.create_new\"}}</span>\n                        </a>\n                    </li>\n                </ul>\n            </div>\n        {{/if}}\n        {{#if theme_settings.show_halo_share}}\n            {{> components/common/share url=product.url layout=\"product-view\"}}\n        {{/if}}\n    </div>\n</div>\n{{#or customer (unless settings.hide_price_from_guests)}}\n    {{#if theme_settings.halo_buy_it_now}}\n        <div class=\"productView-actions productView-actions--buyItNow\">\n            <div class=\"form-action form-action--buyItNow\">\n                <input id=\"form-action-buyItNow\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary button--buyItNow\" type=\"submit\"\n                value=\"{{lang 'products.buy_it_now'}}\">\n            </div>\n        </div>\n    {{/if}}\n{{/or}}\n{{#or customer (unless settings.hide_price_from_guests)}}\n    {{#if this.with_wallet_buttons}}\n    <div class=\"add-to-cart-wallet-buttons\" data-add-to-cart-wallet-buttons>\n        {{> components/common/wallet-buttons}}\n    </div>\n    {{/if}}\n{{/or}}\n"}
